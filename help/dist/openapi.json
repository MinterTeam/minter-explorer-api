  },
  "components": {
    "schemas": {
      "BlockResponse": {
        "properties": {
          "height": {
            "description": "Block height",
            "type": "integer",
            "example": 34863
          },
          "size": {
            "description": "Block size",
            "type": "integer",
            "example": 253
          },
          "txCount": {
            "description": "Transaction count",
            "type": "integer",
            "example": 25
          },
          "blockTime": {
            "description": "Block time",
            "type": "integer",
            "example": 34863
          },
          "timestamp": {
            "description": "Block height",
            "type": "integer",
            "example": 518
          },
          "reward": {
            "description": "Block reward",
            "type": "string",
            "example": "333"
          },
          "hash": {
            "description": "Block hash",
            "type": "string",
            "example": "ITHA0XQPS5XTOY6NTTOOJX5VAVZQHTRIZJDHQKKUPUQDNX7JYVDE4UO5LFFCWSVA"
          },
          "validators": {
            "description": "Block validators",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/BlockValidatorResponse"
            }
          }
        },
        "type": "object"
      },
      "BlockPaginatedCollectionResponse": {
        "properties": {
          "data": {
            "description": "Blocks",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/BlockResponse"
            }
          },
          "links": {
            "$ref": "#/components/schemas/PaginationLinksResponse"
          },
          "meta": {
            "$ref": "#/components/schemas/PaginationMetaResponse"
          }
        },
        "type": "object"
      },
      "CoinResponse": {
        "properties": {
          "crr": {
            "description": "Coin crr",
            "type": "integer",
            "example": 10
          },
          "volume": {
            "description": "Coin volume",
            "type": "string",
            "example": "45656534.3546"
          },
          "reserve_balance": {
            "description": "Coin reserve balance",
            "type": "string",
            "example": "10.356"
          },
          "name": {
            "description": "Coin name",
            "type": "string",
            "example": "My super coin"
          },
          "symbol": {
            "description": "Coin symbol",
            "type": "string",
            "example": "MNT"
          }
        },
        "type": "object"
      },
      "PaginationLinksResponse": {
        "properties": {
          "first": {
            "description": "First page",
            "type": "string",
            "example": "https://testnet.explorer.minter.network/blocks?page=1"
          },
          "last": {
            "description": "Last page",
            "type": "string",
            "example": "https://testnet.explorer.minter.network/blocks?page=10"
          },
          "prev": {
            "description": "Previous page",
            "type": "string",
            "example": "https://testnet.explorer.minter.network/blocks?page=1"
          },
          "next": {
            "description": "Next page",
            "type": "string",
            "example": "https://testnet.explorer.minter.network/blocks?page=2"
          }
        },
        "type": "object"
      },
      "PaginationMetaResponse": {
        "properties": {
          "current_page": {
            "description": "Current page number",
            "type": "integer",
            "example": 2
          },
          "last_page": {
            "description": "Last page number",
            "type": "integer",
            "example": 10
          },
          "path": {
            "description": "Request path",
            "type": "string",
            "example": "https://testnet.explorer.minter.network"
          },
          "per_page": {
            "description": "Elements per page",
            "type": "integer",
            "example": 50
          },
          "total": {
            "description": "Total number of elements",
            "type": "integer",
            "example": 16434
          }
        },
        "type": "object"
      },
      "AddressResponse": {
        "properties": {
          "address": {
            "description": "Minter address",
            "type": "string",
            "example": "Mxe449e9fd85fd553fa45d245b62e30253bce29cc7"
          },
          "balances": {
            "description": "Address balances",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AddressBalanceResponse"
            }
          }
        },
        "type": "object"
      },
      "AddressBalanceResponse": {
        "properties": {
          "coin": {
            "description": "Coin symbol",
            "type": "string",
            "example": "MNT"
          },
          "amount": {
            "description": "Amount of coin",
            "type": "string",
            "example": 1257.4657
          }
        }
      },
      "TransactionResponse": {
        "properties": {
          "txn": {
            "description": "Transaction number",
            "type": "integer",
            "example": 17653
          },
          "hash": {
            "description": "Transaction hash",
            "type": "string",
            "example": "Mtc923c18c1d842dcba262c63df22003d5ee4e683dc2927acf9580f083ad9e193e"
          },
          "nonce": {
            "description": "Transaction nonce",
            "type": "integer",
            "example": 113
          },
          "block": {
            "description": "Block id",
            "type": "integer",
            "example": 119
          },
          "timestamp": {
            "description": "Transaction timestamp",
            "type": "string",
            "example": "2019-02-14T06:45:18.743Z"
          },
          "fee": {
            "description": "Transaction fee",
            "type": "string",
            "example": "10.000000000000000000"
          },
          "type": {
            "description": "Transaction type",
            "type": "string",
            "example": "send"
          },
          "payload": {
            "description": "Transaction payload",
