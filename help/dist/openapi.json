{
  "openapi": "3.0.0",
  "info": {
    "version": "0.0.1",
    "title": "Minter Explorer api"
  },
  "paths": {
    "/api/v1/blocks": {
      "get": {
        "tags": [
          "Blocks"
        ],
        "summary": "Get list of blocks",
        "operationId": "getBlocks",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "Page number",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "description": "Blocks per page",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of blocks",
            "content": {
              "application-json": {
                "schema": {
                  "$ref": "#/components/schemas/BlockPaginatedCollectionResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/blocks/{height}": {
      "get": {
        "tags": [
          "Blocks"
        ],
        "summary": "Get block detail",
        "operationId": "getBlock",
        "parameters": [
          {
            "name": "height",
            "in": "path",
            "description": "Block height",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of blocks",
            "content": {
              "application-json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "$ref": "#/components/schemas/BlockResponse"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Block not found",
            "content": {
              "application-json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "object",
                      "properties": {
                        "code": {
                          "type": "integer",
                          "example": 404
                        },
                        "message": {
                          "type": "string",
                          "example": "Block not found."
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/blocks/{height}/transactions": {
      "get": {
        "tags": [
          "Blocks"
