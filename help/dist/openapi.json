    "/api/v1/addresses/{address}/statistics/rewards": {
      "get": {
        "tags": [
          "Addresses"
        ],
        "summary": "Get reward statistics by Minter address",
        "operationId": "getAddressStatisticRewards",
        "parameters": [
          {
            "name": "address",
            "in": "path",
            "required": true,
            "description": "Minter address",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "scale",
            "in": "query",
            "description": "Chart scale. Possible variants: minute | hour | day. Default is day",
            "schema": {
              "type": "string"
            },
            "example": "minute"
          },
          {
            "name": "startTime",
            "in": "query",
            "description": "Chart start time. Formats: YYYY-MM-DD | YYYY-MM-DD HH:MM:SS",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "endtime",
            "in": "query",
            "description": "Chart end time. Formats: YYYY-MM-DD | YYYY-MM-DD HH:MM:SS",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Chart data",
            "content": {
              "application-json": {
                "schema": {
                  "$ref": "#/components/schemas/RewardStatisticCollectionResponse"
                }
              }
            }
          }
        }
      }
    },
