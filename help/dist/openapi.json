            "schema": {
              "type": "string"
            },
            "example": "minute"
          },
          {
            "name": "startTime",
            "in": "query",
            "description": "Chart start time. Formats: YYYY-MM-DD | YYYY-MM-DD HH:MM:SS. Default is 14 days before.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "endtime",
            "in": "query",
